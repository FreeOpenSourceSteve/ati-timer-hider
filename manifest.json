{
  "manifest_version": 3,
  "name": "ATI Time Hider",
  "version": "1.0",
  "description": "Hides the .timespentlbl element on lms.atitesting.com",
  "icons": {
    "16": "icon_16x16.png",
    "32": "icon_32x32.png",
    "48": "icon_48x48.png",
    "128": "icon_128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://lms.atitesting.com/*"],
      "css": ["styles.css"],
      "js": ["content.js"]
    }
  ]
}
